    <type fullname="*" />
  </assembly>
  
  <!-- 保留 Swashbuckle 相关程序集 -->
  <assembly fullname="Swashbuckle.AspNetCore.SwaggerGen">
    <type fullname="*" />
  </assembly>
  <assembly fullname="Swashbuckle.AspNetCore.SwaggerUI">
    <type fullname="*" />
  </assembly>
</linker>
